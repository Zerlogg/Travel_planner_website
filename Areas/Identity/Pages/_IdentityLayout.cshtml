@namespace TravelingTrips.Areas.Identity.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
</head>
<body style="overflow: hidden;">
    <div class="full-page-background" style="position: fixed; top: 0; right: 0; bottom: 0; left: 0">
        <img Src="Images/Travel.jpg" Alt="Travel" style="width: 100%; height: 100%; object-fit: cover"/>
    </div>
    <div style="position: relative">
        @RenderBody()
    </div>
</body>
</html>